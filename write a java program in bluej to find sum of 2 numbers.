public class SumOfTwoNumbers {
    public static void main(String[] args) {
        // Declare and initialize the two numbers
        int num1 = 10;
        int num2 = 20;

        // Calculate the sum of the two numbers
        int sum = num1 + num2;

        // Print the sum
        System.out.println("Sum of " + num1 + " and " + num2 + " is: " + sum);
    }
}
